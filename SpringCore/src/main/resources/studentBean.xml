<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="stu" class="com.java.ioc.beanfactory.Student">
        <property name="name" value ="hari"></property>
        <property name="id" value="10"></property>
        <property name="address" value="bangalore"></property>
    </bean>

    <bean id="stu1" class="com.java.ioc.beanfactory.Student">
        <property name="name" value="emixo"></property>
        <property name="id" value="20"></property>
        <property name="address" value="hyderabad"></property>
    </bean>
</beans>